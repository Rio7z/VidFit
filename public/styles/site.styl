@import "bootstrap"


@import "site/_variables"


// Site Styles
// ===========
body
  padding-top 20px

.videoWrapper
	position: relative
	padding-bottom: 56.25% /* 16:9 */
	padding-top 25px
	height 0px
  margin-bottom: 10px

.videoWrapper iframe
	position absolute
	top 0
	left 0
	width 100%
	height 100%
a:link
  text-decoration none!important
.hover
  transition all 0.5s
  &:hover
    transform: scale(1.1);
.top-space
  padding-top: 8px
.nav
  margin-bottom: 0px
.hero
  color: white
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  display: flex;
  height: 100vh!important
  justify-content: center;
  align-items: center
.hero-1
  background-image: url("../images/hero1.jpg");
.hero-2
  background-image: url("../images/hero2.jpg");
.video-items
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  align-items: flex-start;
  div
    justify-content: center;
    flex-wrap: wrap;
    width 200px
    margin: 0 10px 10px 10px
    img
      width 100%
      height auto
.product-items
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  align-items: flex-start;
  div
    justify-content: center;
    width 300px
    margin: 0 10px 10px 10px
    img
      width 100%
      height auto
.product-h
  height: 60%
  align-items: center;
.nav-logo
  width: 10%
.nav-logosm
  width: 53px
a
  color: #111
a:hover
  color: #111
.pagination
  .active a
    background-color: #111
    border-color #111
  .active a:hover
    background-color: #111
    border-color #111
  li
    a
      color: #111
    a:hover
      color: #111
    a:focus
      color: #111
.line
  padding-top 10px
.blackbar
  background-color: #111
  padding 5px
#footer
  display: flex;
  justify-content: center;
  height: 100px;
  background-color: #f2f2f2
  color: #777
  text-align: center;
// Add your own site style includes here
@import "site/_layout"
